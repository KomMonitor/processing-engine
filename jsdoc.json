{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
			"include": [
				"resources/KmHelper"
			],
			"includePattern": ".+\\index.js(doc)?$",
			"excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "template": "node_modules/docdash",
        "encoding": "utf8",
        "destination": "kmHelperAPIdocs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
